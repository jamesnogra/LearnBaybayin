<?xml version='1.0' encoding='utf-8'?>
<widget id="com.iamcebu.learnbaybayin" version="2.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Learn Baybayín</name>
    <description>
        An eLearning application for the Baybayín Script. Built using the Apache Cordova platform.
    </description>
    <author email="love.gerald@yahoo.com" href="https://learnbaybayin.iamcebu.com/">
        Learn Baybayín Team
    </author>
    <widget version="1.0.22" />
    <icon src="res/icon/baybayin-icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="geo:*" />
    <preference name="orientation" value="portrait" />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name="android-minSdkVersion" value="23" />
</widget>
