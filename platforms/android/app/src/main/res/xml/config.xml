<?xml version='1.0' encoding='utf-8'?>
<widget id="com.iamcebu.learnbaybayin" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <name>Learn Baybayín</name>
    <description>
        An eLearning application for the Baybayín Script. Built using the Apache Cordova platform.
    </description>
    <author email="love.gerald@yahoo.com" href="https://learnbaybayin.iamcebu.com/">
        GetLoud Tech
    </author>
    <widget version="1.0.4" />
    <icon src="res/icon/baybayin-icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="orientation" value="portrait" />
</widget>
